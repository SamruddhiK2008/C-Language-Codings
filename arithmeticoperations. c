
#include<stdio.h>

int main() {
     int a,b;
     a=13;
     b=17;
     //operationforaddition//
     printf("a+b=%d\n",a+b);
     //operationforsubtraction//
     printf("a-b=%d\n",a-b);
     //operationformultiplication//
     printf("a*b=%d\n",a*b);
     //operationfordivision//
     printf("a/b=%d\n",a/b);
     //operationformodulus//
     printf("a%b=%d\n",a%b);
     return 0;
     }
     
    output:-
a+b=30
a-b=-4
a*b=221
a/b=0    